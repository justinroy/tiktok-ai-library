# Base URL of the backend API used for production builds
VITE_API_BASE_URL=https://your-production-backend.example.com

# Optional: override the default dev server port (5173)
VITE_DEV_SERVER_PORT=5173

# Target backend for local development proxying (e.g., Streamlit or FastAPI server)
VITE_DEV_PROXY_TARGET=http://localhost:8501

# Optional: override the preview server port (4173)
VITE_PREVIEW_PORT=4173
